{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }, // SMTP
  "AllowedHosts": "*",
  "SmtpSettings": {
    "Server": "smtp.gmail.com",
    "Port": 587,
    "SenderName": "Amecys",
    "SenderEmail": "%SMTP_SENDER_EMAIL%",
    "Username": "%SMTP_USERNAME%",
    "Password": "%SMTP_PASSWORD%",
    "RecipientEmail": "%SMTP_RECIPIENT_EMAIL%"
  }
}
